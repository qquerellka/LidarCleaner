{
  "name": "pcd-viewer",
  "private": true,
  "version": "0.1.0",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "dev:renderer": "vite",
    "dev:main": "esbuild src/main/index.ts --bundle --platform=node --outdir=dist-electron/main --format=cjs --sourcemap --watch --external:electron",
    "dev:preload": "esbuild src/main/preload.ts --bundle --platform=node --outdir=dist-electron/preload --format=cjs --sourcemap --watch --external:electron",
    "dev:electron": "wait-on tcp:5173 && electron .",
    "dev": "concurrently -r -n RENDERER,MAIN,PRELOAD,ELECTRON -c yellow,cyan,magenta,green \"npm:dev:renderer\" \"npm:dev:main\" \"npm:dev:preload\" \"npm:dev:electron\"",
    "build": "vite build",
    "preview": "electron .",
    "start": "npm run dev"
  },
  "devDependencies": {
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/react-redux": "^7.1.34",
    "@types/three": "^0.180.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^9.2.1",
    "electron": "^30.0.9",
    "electron-log": "^5.2.0",
    "esbuild": "^0.25.10",
    "typescript": "^5.5.0",
    "vite": "^5.4.0",
    "vite-plugin-electron": "^0.28.0",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^9.0.1"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "axios": "^1.7.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.2.0",
    "three": "^0.180.0"
  }
}
